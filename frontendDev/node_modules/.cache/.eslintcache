ut);\n\n    if(resp){\n      setRole(resp[0]);\n      setFlag(true);\n    }\n  }\n\n  useEffect(()=>{if(flag)navigate('/')});\n\n\n  return (\n    <>\n\n    <div className=\"logo\">\n    <img src=\"logo\" id=\"logologin\"/>\n    </div>\n\n    <div className=\"box\">\n\n      <div className = \"loginbox\">\n        <div className='bemvindo'>\n          <p>Bem Vindo</p>\n        </div>\n        <Form onSubmit={handleSubmit}>\n          <input type=\"text\" name=\"Email\" placeholder=\"Email\" onChange={handleChange}/>\n          <input type=\"password\" name=\"PlvPasse\" placeholder=\"Palavra-passe\" onChange={handleChange}/>\n          <button className = \"button\" type=\"submit\">Login</button> \n        </Form>\n        <p>Não tem conta?</p>\n        <a href='register'>Registe-se já!</a>\n      </div>\n\n      <div className = \"loginimg\">\n        <img src=\"loginimage.jpg\" id=\"loginimage\"/>\n      </div>\n      \n      \n    </div>\n    \n    </>\n  );\n}","/home/setefi/RAS/RASBet2022/frontendDev/src/routes/index.jsx",[],[],"/home/setefi/RAS/RASBet2022/frontendDev/src/routes/register.jsx",["123","124"],[],"import {Link ,Form,redirect} from 'react-router-dom'\nimport axios from 'axios'\nimport './login.css'\n\nimport {setToken} from \"../utils\"\n\n\nexport async function action({request,params}){\n  const data = await request.formData();\n  const updates = Object.fromEntries(data);\n  console.log(updates);\n  var flag = await axios({method:'POST',url:'http://localhost:8080/api/register/',data:updates}) \n  .then(function (response) {\n    console.log(response);\n    const data = response.data;\n    setToken(data.Token);\n    return true;\n  })\n  .catch(function (error) {\n    console.log(error,flag);\n    return false;\n  });\n  \n\n  console.log(flag);\n  if(flag){\n    return redirect(\"/\");\n  }\n}\n\nexport default function Register() {\n\n  return (\n    <>\n      <h1>Pagina de registo de utilizador</h1>\n\n      <div className = \"box\">\n        <di